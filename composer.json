{
    "name": "mphpmaster/nova-info-card",
    "description": "Laravel Nova Resource Information Card.",
    "type": "library",
    "homepage": "https://github.com/mPhpMaster/nova-info-card",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mphpmaster\\NovaInfoCard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mPhpMaster",
            "email": "mphpmaster@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "^8.54",
        "laravel/helpers": "^1.4",
        "laravel/nova": "*",
        "spatie/laravel-medialibrary": "^8.0|^9.0",
        "spatie/laravel-translatable": "^4.6",
        "spatie/laravel-translation-loader": "^2.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mphpmaster\\NovaInfoCard\\CardServiceProvider"
            ]
        },
        "aliases": {
            "Mphpmaster\\InfoCard": "Mphpmaster\\NovaInfoCard"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
